      "source": [
        "keras.layers.MaxPooling2D(2, strides=2, padding='valid')   # strides의 기본값은 자동으로 풀링의 크기이므로 지정할 필요X,\n",
        "#                                                            padding도 기본으로 valid이므로 지정할 필요X"
      ],
      "execution_count": 5,
      "outputs": [
        {
          "output_type": "execute_result",
